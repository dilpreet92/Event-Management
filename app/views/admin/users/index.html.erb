<br>
<% if @users.empty? %>
  <h3 class="text-center mtop55 text-blue back-blue p5 wraper"> No User Exists </h3>
<% else %>
  <%= will_paginate @users, remote: true %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Twitter Handle</th>
        <th>Enabled</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <%= render @users %>
    </tbody>
  </table>
<% end %>
<br>