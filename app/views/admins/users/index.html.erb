<br>
<% if @users.empty? %>
  <h4>No User Exists </h4>
<% else %>   
  <%= will_paginate @users, remote: true %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Twitter Handle</th>
        <th>Enabled</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <%= render @users %>
    </tbody>
  </table>
<% end %>
<br>