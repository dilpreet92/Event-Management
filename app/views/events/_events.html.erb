<br>
<% if @events.empty? %>
  <h4>No Such Events </h4>
<% else %>   
  <%= will_paginate @events %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>Start date</th>
        <th>End date</th>
        <th>Address</th>
        <th>City</th>
        <th>Country</th>
        <th>Contact number</th>
        <th>Description</th>
        <th colspan="3"></th>
      </tr>
    </thead>
    <tbody>
      <%= render @events %>
    </tbody>
  </table>
<% end %>
<br>